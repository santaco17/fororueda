<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FrontController</servlet-name>
        <servlet-class>Control.FrontController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnknownCommandView</servlet-name>
        <servlet-class>view.UnknownCommandView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogInCommandView</servlet-name>
        <servlet-class>view.LogInCommandView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnuncioCommandView</servlet-name>
        <servlet-class>view.AnuncioCommandView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerHilosCommandView</servlet-name>
        <servlet-class>view.VerHilosCommandView</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FrontController</servlet-name>
        <url-pattern>/FrontController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnknownCommandView</servlet-name>
        <url-pattern>/UnknownCommandView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogInCommandView</servlet-name>
        <url-pattern>/LogInCommandView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnuncioCommandView</servlet-name>
        <url-pattern>/AnuncioCommandView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerHilosCommandView</servlet-name>
        <url-pattern>/VerHilosCommandView</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
